{{#if results}}
	<p>results {{results}}</p>
	<p>this.results.name {{this.results.name}}</p>
	<p>this.name {{this.name}}</p>
	<p>results.name {{results.name}}</p>
	<p>this.Results: {{this.results}}</p>
	<p>Context: {{context}}</p>
{{/if}}
<form action="/" method="post">
	<label for="name">Name of Exercise:</label>
	<input id="name" type="text" name="name" value="{{this.results.name}}">
	
	<label for="reps">Number of Reps:</label>
	<input id="reps" type="text" name="reps" value={{this.results.reps}}>

	<label for="weight">Weight:</label>
	<input id="weight" type="text" name="weight" value={{this.weight}}>

	<label for="date">Date:</label>
	<input id="date" type="text" name="date" value={{this.dateOccured}}>

	{{#if this.lbs}}
		<input type="radio" name="lbs" value=1 checked>lbs<br>
	    <input type="radio" name="lbs" value=0>kg<br>
	{{/if}}

	{{#if this.lbs}}
		<input type="radio" name="lbs" value=0>lbs<br>
	    <input type="radio" name="lbs" value=1 checked>kg<br>
	{{/if}}
	<input type="submit" name="update">
</form>
